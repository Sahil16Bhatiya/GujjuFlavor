    border-radius: 50px;
    font-weight: 600;
    width: 300px;
  }


   .target {
  position: relative;
  top: -200px;
  max-width: 750px;
  margin: 0 auto;
  transform: translatey(30vh);
  font-size: 16.5px;
  color: rgb(255, 255, 255);
  font-weight: 700;
  text-shadow: 1px 2px black;
  line-height: 1.75rem;
}

input {
  position: absolute;
  bottom: 0;
  display: none;
}

.demo-cursor {}

@keyframes move {
  100% {
    transform: translate(0, 0);
  }
}

@keyframes remove {
  100% {
    color: red;
    transform: rotate(20deg) scale(0.1) translate(0, -200px);
    opacity: 0;
  }
}
